{
  "name": "abzen",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "concurrently \"npm:start-grunt\" \"npm:start-serve\" \"npm:start-nx\"",
    "start-grunt": "grunt watch",
    "start-serve": "tsc --build --watch apps/serve/tsconfig.app.json ",
    "start-nx": "nx run-many --target=serve --all --parallel",
    "start:docker": "docker build -t abzen . && docker run --rm --env-file=docker.env -it -p 4000:4000 abzen",
    "build": "nx affected:build --parallel",
    "build:admin": "nx build admin-server && nx build admin-client",
    "build:serve": "tsc --build apps/serve/tsconfig.app.json && jsmin -o dist/apps/serve/abzen.js dist/apps/serve/main.js",
    "lint": "nx affected:lint --parallel",
    "test": "nx test",
    "e2e": "nx e2e",
    "ts": "npx ts-node --skip-project -O \"{\\\"esModuleInterop\\\":true,\\\"experimentalDecorators\\\": true,\\\"emitDecoratorMetadata\\\":true, \\\"target\\\":\\\"es6\\\"}\"",
    "admin-server:migrate": "yarn run ts apps/admin-server/scripts/migrate.ts",
    "admin-server:create-user": "yarn run ts apps/admin-server/scripts/create_user.ts"
  },
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@godaddy/terminus": "^4.3.1",
    "apollo-boost": "^0.4.7",
    "apollo-server": "^2.25.3",
    "apollo-server-express": "^2.10.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "commander": "^4.1.1",
    "connect-redis": "^4.0.4",
    "convict": "^5.2.0",
    "cors": "^2.8.5",
    "document-register-element": "1.13.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-winston": "^4.0.3",
    "graphql": "^14.6.0",
    "graphql-relay": "^0.6.0",
    "moment": "^2.24.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^7.18.2",
    "pug": "^2.0.4",
    "rc-slider": "^9.2.3",
    "react": "16.12.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.5",
    "sequelize-typescript": "^1.1.0",
    "umzug": "^2.2.0",
    "uuid": "^7.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/helper-validator-identifier": "^7.9.0",
    "@nrwl/cypress": "9.0.4",
    "@nrwl/eslint-plugin-nx": "9.0.2",
    "@nrwl/jest": "9.0.2",
    "@nrwl/node": "^9.0.2",
    "@nrwl/react": "^9.0.4",
    "@nrwl/web": "9.0.4",
    "@nrwl/workspace": "9.0.2",
    "@testing-library/react": "9.4.0",
    "@types/bluebird": "^3.5.30",
    "@types/commander": "^2.12.2",
    "@types/connect-redis": "^0.0.13",
    "@types/convict": "^4.2.1",
    "@types/express-session": "^1.15.16",
    "@types/express-winston": "^4.0.0",
    "@types/graphql-relay": "^0.4.11",
    "@types/jest": "24.0.9",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.7.4",
    "@types/passport": "^1.0.2",
